{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "XRay": {
    "DisableXRayTracing": "false",
    "UseRuntimeErrors": "true",
    "CollectSqlQueries": "false"
  },
  "AWS": {
    "Region": "eu-west-2"
  },
  "DynamoDb": {
    "LocalMode": false
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Properties": {
      "Application": "HousingRegisterApi"
    }
  },
  "AllowedHosts": "*"
}
